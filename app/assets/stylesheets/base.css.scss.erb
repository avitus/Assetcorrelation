	display:none;
	font-weight:normal;
	font-style:normal;
	z-index:2;	
	position:absolute;
	top:50px;
	left:410px; /* Should match 'hide' position in javascript on add deal page */
	text-align:left;
	width: 250px;
    background-color:#1F2229; 
    outline:1px solid #669; 
    border:2px solid #fff; 
    padding:10px 15px; 
    color:#fff;  
    font-size:100%; 		
}

/* === User stats === */

.user-stats-table {
	float:left;
	margin:0.2em;
}

/* === Styling for test feedback === */

 /* 1st Panel */

#priordeal {
	color: #444444;  /* gray */
	padding-bottom: 16px;
}

#currentdeal {
	position: relative;
	width: 290px;	
}

#complete-hint {
	display: none;
	width: 290px;	
}

div#currentdeal div#matchbox {
	position: absolute;
	bottom: -60px;
}

div#currentdeal #matchbox p {
	font-size: 130%;
   	text-align: center;		
	color: #99cc33;  /* light green */	
	background: #404040; /* dark gray */
	padding: 0.2em 1em 0.2em 1em;
	margin: 0 0 0 85px;		
}

div#currentdeal #matchbox .incorrect {
	background: #687284; /* light green */
	font-size: 200%;
	font-style: italic;
}

div#currentdeal #matchbox .correct {
	background: #687284; /* light green */
	font-size: 500%;	
	font-style: italic;
}

/* === Styling for Flashcard === */

.mnemonic
{
	background:#ffc;
	border:1px solid #ccc;
	padding:1em;
	font-weight:bold;
	color:#666;
	text-align:center;
	margin-bottom:8px;
}

/* === Tweets === */
div.tweet {
	padding-top: 0.2em;
	padding-bottom: 0.2em;
}

.tweet .time {
	color: #606060;
	font-size: 80%
}


/* === Error Messages === */
div#errorExplanation {
  width: 400px;
  border: 2px solid red;
  padding: 7px;
  padding-bottom: 12px;
  margin-bottom: 20px;
  background-color: #f0f0f0;	
}

div#errorExplanation h2 {
  text-align: left;
  font-weight: bold;
  padding: 5px 5px 5px 15px;
  font-size: 12px;
  margin: -7px;
  background-color: #c00;
  color: #fff;	
}

div#errorExplanation p {
  color: #333;
  margin-bottom: 0;
  padding: 5px;	
}


div#errorExplanation ul li {
  font-size: 12px;
  list-style: square;	
}


/* === Login Page === */

#login-page-form {
	width:540px;
	margin-left:50px;
	margin-top:0.25em;
	padding-top:2em;
	float:left;
	font-size:150%;
}

#login-page-form label {
	width:7em;
}

#login-page-form li#remember-me label {
	font-size:100%;
}

#intro-description {
	margin-top:0.1em;
	margin-left:25px;
	float:left;
	width:500px;
	height:400px;
	font-size:125%;
}

#intro-description li {
	font-size:100%;
}


/* === Signup Page === */

#signup-page-form {
    font-size:140%;
	width:640px;
	margin-left:7%;
	margin-top:1.5em;
}

#signup-page-form label {
	width:8.5em;
}

#open-id-link {
	font-size:80%;
	font-style:italic;
	padding-left:1em;	
}

/* === Forgotten password page (request email to be sent) === */

#forgotten_password {
	width:550px;
}

#forgotten_password label {
	width:2.5em;
}

/* === Reset password page === */

#reset_password {
	width:550px;
}

#reset_password label {
	width:5.5em;
}

#reset_password .buttons {
	margin-left:9em;
}

/* === Edit Profile Page === */

#edit-profile-form {
	width:600px;
}

#edit-profile-form label {
	width:15em;  /* changed from 12em on March 6 2010 */
	padding-right:0.5em;
}

#edit-profile-form li {
	padding-top:0.12em;
	padding-bottom:0.12em;
	width:570px;
	list-style:none;
}

#edit-profile-form .buttons {
	margin-left:12em;
	padding-top:0.25em;
	margin-bottom:0.7em;
}

.autocomplete-w1 {background:url(<%= asset_path "shadow.png" %>) no-repeat bottom right;position:absolute;top:0;left:0;margin:8px 0 0 6px; /* IE6 fix: */ _background:none; _margin:0; }
.autocomplete {border:1px solid #999;background:#FFF;cursor:default;text-align:left;max-height:350px;overflow:auto; margin:-6px 6px 6px -6px; /* IE6 specific: */ _height:350px;  _margin:0; _overflow-x:hidden; }
.autocomplete .selected {background:#afaeae;} /* Background color for selected option */
.autocomplete div {padding:2px 5px;white-space:nowrap;}
.autocomplete strong {font-weight:normal;color:#3c5313;}  /* Color for matching text */

/* === Referrals Page === */
.referrals-section {
	margin-top: 1.5em;
}

/* === Styles for home page === */
.start-page-actions {
	float:left;
	width:500px;
	height:220px;	
}

#getting_started {
	float:left;
	width:800px;
	height:420px;	
}

.start-page-actions ol li {
	font-size:120%;
	list-style-type:decimal;	
}


#volunteer a:link {
	text-decoration:underline;
}


/* === Entry Forms === */

.entry-form {
	width:360px;	
}

.entry-form fieldset {
	background-color:#eee; /* light green */
}

.entry-form legend {
	background:#ccc; /* dark green */
	color:#222; /* lightest green */
	padding:0.2em 1em;
}

.entry-form label {
	width:6em;
	float:left;
	text-align:right;
	margin-right:0.5em;
	display:block;
}

.entry-form ul li {
	list-style: none;
}

.entry-form .submit {
	margin-left:2.5em;
}

.entry-form .buttons {
	margin-left:2.5em;
}

/* === Forms: Errors === */

div.fieldWithErrors {
  padding:2px;
  background-color:red;
  display:table;	
}

.input-error {
  background-color:#aa0101;
  color:white;
}


